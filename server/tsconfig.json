{
  "compilerOptions": {
    "strict": true,

    // "baseUrl": "./src",
    "outDir": "./dist",

    "target": "es2020",
    "module": "esnext"  ,
    "moduleResolution": "node",

    "esModuleInterop": true,
    "skipLibCheck": true,

    "paths": {
      "#lib/*": ["./lib/*"],
      "#app/*": ["./src/app/*"],
      "#utils/*": ["./src/utils/*"],
      "#config": ["./src/config/index.ts"],
      // "#database": ["./src/database/index.ts"],
      // "#model": ["./src/model/index.ts"],
      // "#constants": ["./constants"],
      // "#helpers": ["./helpers"],
      // "#utils": ["./utils"],
    }
  },
  "include": ["./src", "./lib", "tsdown.config.ts"],
  "exclude": ["node_modules"]
}
